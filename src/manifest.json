{
  "name": "网页体验助手",
  "description": "还我网页体验！！！！",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://www.imeijutt.com/vodplay/*"],
      "css": ["mjtt/index.css"],
      "js": ["mjtt/index.js"]
    },
    {
      "run_at": "document_end",
      "matches": [
        "http://livingroom.cmc.zju.edu.cn/*",
        "http://vod.cmc.zju.edu.cn/*"
      ],
      "css": ["zykt/index.css"],
      "js": ["zykt/index.js"]
    }
  ]
}
